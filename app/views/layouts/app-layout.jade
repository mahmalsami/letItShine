include ../mixins/all

block variables

- if(typeof title === "undefined") title="Le Groupe Made"
- if(typeof appcache === "undefined") appcache=null

doctype html
//[if IE 8]><html lang="fr" class="no-js old-ie ie8"><![endif]
//[if IE 9]><html lang="fr" class="no-js old-ie ie9"><![endif]
//[if gt IE 9]><!
html(class="no-js", lang="fr", appcache=appcache)
  //<![endif]
  head
    meta(charset="utf-8")
    title=title
    //[if lt IE 9]><meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"><![endif]
    meta(name="description", content="")
    meta(name="viewport", content="user-scalable=no, initial-scale=1, minimum-scale=1, maximum-scale=1, width=device-width")

    // build:css stylesheets/app.min.css
    link(rel="stylesheet", href="stylesheets/app.css")
    // endbuild

    link(rel="apple-touch-icon-precomposed", href="images/icons/apple-touch-icon.png")
    link(rel="shortcut icon", href="favicon.png")

    meta(property="og:site_name", content="")
    meta(property="og:title", content="")
    meta(property="og:url", content="")
    meta(property="og:description", content="")

    // build:js javascripts/libs/head.min.js
    script(src="javascripts/libs/head.js")
    // endbuild

  body

    #app
      include ../common/header
      block body-content
      include ../common/footer

    // build:js javascripts/libs/vendor.min.js
    script(src="javascripts/libs/vendor.js")
    // endbuild

    script(src="/socket.io/socket.io.js")


    // build:js javascripts/app.min.js
    script(src="javascripts/app.js")
    // endbuild
